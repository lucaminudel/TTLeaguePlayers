{
  "name": "ttleagueplayersap_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "copy-config": "node scripts/copy-config.js",
    "dev": "npm run copy-config && vite",
    "test": "npm run copy-config && cross-env ENVIRONMENT=test vite --port 4173",
    "build": "npm run build:dev",
    "build:steps": "npm run copy-config && npm run lint && tsc -b && vite build",
    "build:dev": "cross-env ENVIRONMENT=dev npm run build:steps",
    "build:test": "cross-env ENVIRONMENT=test npm run build:steps",
    "build:staging": "cross-env ENVIRONMENT=staging npm run build:steps",
    "build:prod": "cross-env ENVIRONMENT=prod npm run build:steps",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-compiler-runtime": "^1.0.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "babel-plugin-react-compiler": "^1.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-dom": "^2.3.13",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-react-x": "^2.3.13",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}