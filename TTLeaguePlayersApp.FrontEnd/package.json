{
  "name": "ttleagueplayersap_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "copy-config": "node scripts/copy-config.js",
    "lint": "eslint .",
    "build:steps": "npm run copy-config && npm run lint && tsc -b && vite build",
    "run-web:dev-env": "npm run copy-config && vite",
    "run-web:test-env": "npm run copy-config && cross-env ENVIRONMENT=test vite --port 4173",
    "build-web": "npm run build:dev",
    "build-web:dev-env": "cross-env ENVIRONMENT=dev npm run build:steps",
    "build-web:test-env": "cross-env ENVIRONMENT=test npm run build:steps",
    "build-web:staging-env": "cross-env ENVIRONMENT=staging npm run build:steps",
    "build-web:prod-env": "cross-env ENVIRONMENT=prod npm run build:steps",
    "unit-tests-web:run": "vitest run",
    "unit-tests-web:browser-run": "vitest --ui --open",
    "e2e-tests-web:run": "playwright test",
    "e2e-tests-web:report": "playwright show-report",
    "e2e-tests-web:browser-run": "playwright test --ui"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-compiler-runtime": "^1.0.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "babel-plugin-react-compiler": "^1.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-dom": "^2.3.13",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-react-x": "^2.3.13",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  }
}
